on boot
    # lmkd properties
    setprop ro.config.low_ram false
    setprop ro.lmk.kill_timeout_ms 200

on property:sys.post_boot.parsed=1
    setprop vendor.post_boot.parsed 1

on property:vendor.post_boot.parsed=1
    exec u:r:magisk:s0 root root -- ${MAGISKTMP}/init.mcd.swap.sh
    exec u:r:magisk:s0 root root -- ${MAGISKTMP}/init.mcd.post_boot.sh
